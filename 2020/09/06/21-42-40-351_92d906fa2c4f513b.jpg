	<script type="text/javascript">

		$(document).ready(function(){
		
			if($(window).width() <= 992) {
				
				$("#topmenu .collapse").attr("style", "display: none !important");
			
				$(".container.main").after("<div id=\"mobile-sidebar\" style=\"display:none\"></div>");
		
				var sidebar_menu = "<div class=\"mobile-nav\">" + $("#topmenu .collapse").html() + "</div>";
				var sidebar_content = "<br>";
				if ($("#sidebar-content").html() !== undefined) sidebar_content += $("#sidebar-content").html();
				$("#mobile-sidebar").html(sidebar_menu + sidebar_content);
		
				$("#topmenu .navbar-toggle").after('<button type="button" class="mobile-toggler navbar-toggle navbar-right"><span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span></button>');
				
				$(".mobile-toggler").on("click", function(){
				
					window.scrollTo(0, 0);
					var wwidth = $(window).width();
		
					if( $("#mobile-sidebar").is(":visible") ) {

						$(".container.main").show();
						$("#mobile-sidebar").animate({ "margin-left": "-"+wwidth+"px", "margin-right": wwidth+"px" }, "fast", function(){
							$("#mobile-sidebar").hide();
						});
					}
					else {
						$("#mobile-sidebar").show();
						$("#mobile-sidebar").animate({ "margin-left": "0px", "margin-right": "-20px" }, "fast", function(){
							$(".container.main").hide();
							
						});
					}
				});
			}

		});
	</script>
